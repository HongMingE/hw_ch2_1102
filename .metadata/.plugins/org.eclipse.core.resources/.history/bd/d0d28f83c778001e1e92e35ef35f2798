package hw2;

public class sample23
{
	public static void main(String[] args)
	{
	
		int[][]test;
		test = new int[2][5];
		
		test[0][0]=80;
		test[0][1]=60;
		test[0][2]=22;
		test[0][3]=50;
		test[0][4]=75;
		test[0][0]=90;
		test[0][1]=55;
		test[0][2]=68;
		test[0][3]=72;
				
		for(int i=0;i<test[1].length;i++)
		{
			System.out.println("第" +(i+1)+ "個人的國語分數是"+test[0][i]+"分");
			System.out.println("第" +(i+1)+ "個人的數學分數是"+test[0][i]+"分");
		}
	}
}